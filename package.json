{
  "name": "kitze-js-helpers",
  "description": "Helper functions for easier js development",
  "main": "src/index.js",
  "version": "0.0.0-semantically-released",
  "scripts": {
    "test": "mocha ./test/**/*.js -w",
    "test:single": "istanbul cover -x *.test.js _mocha -- -R spec ./test/**/*.js",
    "commit": "git cz",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kitze/kitze-js-helpers.git"
  },
  "keywords": [
    "kitze",
    "javascript",
    "helpers",
    "js",
    "lodash",
    "underscore"
  ],
  "author": "kitze",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kitze/kitze-js-helpers/issues"
  },
  "homepage": "https://github.com/kitze/kitze-js-helpers#readme",
  "dependencies": {
    "lodash": "^3.10.1",
    "underscore.string": "^3.2.2"
  },
  "devDependencies": {
    "chai": "^3.3.0",
    "codecov.io": "^0.1.6",
    "commitizen": "^1.0.5",
    "cz-conventional-changelog": "^1.1.2",
    "istanbul": "^0.3.21",
    "mocha": "^2.3.3",
    "semantic-release": "^4.3.5"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:single && npm run check-coverage"
    }
  }
}
